class Professor {
  - id : Long
  - cursos: List<Curso>
}

class Aluno {
  - id : Long
  - matriculas: List<Matricula>
}

class Aula {
  - id : Long
  - matricula: Matricula
  - conteudos: List<CONTEUDO>
  - repertorio: List<Repertorio>
  - atividades: List<Atividade>
}

class Atividade{
  - objetivos : List<ObjetivosEspecificos>
}

class Matricula {
  - id : Long
  - curso : Curso
  - inicio: DateTime
  - fim: DateTime
}

abstract Pessoa {
  - nome : String
  - sobrenome : String
  - endere√ßo : Endereco
  - usuario: String
  - senha: String
  - contato: Contato
  - endereco: Endereco
}

class Contato {
  - email: String
  - telefone: String
}

class Endereco{
 -String: logradouro
 -int: numero
 -String: complemento
 -String: cep
 -String: cidade
 -String: estado
 -String: pais
}

class Curso {
  -nome: String
  -modulos : List<Modulo>
  -categorias : List<Categoria>
  -recursos: List<Recursos>
}

class Categoria {
  - id : Long
  - nome : String
}

class Modulo {
  -nome: String
  -unidades : List<UnidadeDidatica>
}

abstract Objetivo{
  -id: Long
  -verbo: String
  -complemento: String
}

class Metodologia{
  -id : Long
  -acao: String
  -conteudo: String
  -modo: String
}

class UnidadeDidatica{
  -id : Long
  -aulas: List<Aula>
  -nome: String
  -conteudos: List<Conteudos>
  -referencias: List<String>
  -questionarios: List<Questionario>
}

class Tema{
  -id : Long
  -nome: String
  -repertorio: List<Repertorio>
  -areasRelacionadas: List<Areas>
}

class Repertorio{
  -id : Long
  -nomeObra: String
  -conteudos: List<CONTEUDO>
  -autor: String
  -genero: String
  -tom: String
  -ano: DateTime
}

class Atividade{
  -id : Long
  -nome: String
  -descricao: String
  -objetivos : List<ObjetivoEspecifico>
  -acoes: List<AcaoImpl>
}

